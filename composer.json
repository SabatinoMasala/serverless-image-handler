{
    "name": "sabatino/serverless-image-handler",
    "description": "Helper class for the Serverless Image Handler on AWS",
    "keywords": [
        "SabatinoMasala",
        "Image resizer"
    ],
    "homepage": "https://github.com/SabatinoMasala/serverless-image-handler",
    "license": "MIT",
    "authors": [
        {
            "name": "SabatinoMasala",
            "email": "masalasabatino@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "autoload": {
        "psr-4": {
            "Sabatino\\ServerlessImageHandler\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sabatino\\ServerlessImageHandler\\ServerlessImageHandlerServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
